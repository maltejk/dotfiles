[include]
	path = ~/.gitconfig.local
[pull]
	rebase = true
[fetch]
	prune = true
[grep]
	extendedRegexp = true
[alias]
	p = ! git pull && git push
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[core]
	excludesfile = ~/.gitignore
[diff]
	noprefix = true
[init]
	defaultBranch = main
[push]
	autoSetupRemote = true
[gpg]
	format = ssh
[user]
	signingkey = /Users/maltejk/.ssh/id_ed25519
[commit]
	gpgsign = true
[advice]
	skippedCherryPicks = false
[tag]
	gpgsign = true
